#!/usr/bin/env bash

set -e
cd $(dirname ${BASH_SOURCE[0]})

function run {(
  mvn clean compile exec:java
)}

function fatjar {(
  mvn clean package
)}

function help {(
  echo "Available commands:"
  for fn in $(compgen -A function); do echo "  ${fn}"; done
)}

[ -z "$1" ] && help && exit 0
"$1" ${@:2}
